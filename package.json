{
  "name": "pith",
  "type": "module",
  "version": "4.0.1",
  "description": "JavaScript SVG parser and renderer on Canvas.",
  "authors": [
    {
      "name": "Liam Parker",
      "email": "liam@spiff.com.au"
    },
    {
      "name": "Joshua Jones",
      "email": "josh@spiff.com.au"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spiffdev/pith"
  },
  "bugs": {
    "url": "https://github.com/spiffdev/pith/issues"
  },
  "keywords": [
    "javascript",
    "client",
    "browser",
    "svg",
    "canvas"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "sideEffects": false,
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "directory": "package"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "NODE_ENV=development rollup -c --watch & node --loader ts-node/esm test/common/server.ts",
    "start:node": "ts-node -P ./tsconfig.dev.json test/node/index.ts",
    "dev": "vite",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "@types/raf": "^3.4.0",
    "raf": "^3.4.1",
    "rgbcolor": "^1.0.1",
    "stackblur-canvas": "^2.0.0",
    "svg-pathdata": "^6.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^11.1.4",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@swc/core": "^1.2.136",
    "@swc/helpers": "^0.3.2",
    "@swc/jest": "^0.2.17",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.16",
    "@types/node-fetch": "^2.5.12",
    "@types/puppeteer": "^5.4.2",
    "@types/xmldom": "^0.1.29",
    "browserslist": "^4.19.1",
    "canvas": "^2.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-html": "^7.1.0",
    "node-fetch": "^2.6.0",
    "puppeteer": "^13.0.0",
    "rollup": "^2.67.1",
    "rollup-plugin-swc": "^0.2.1",
    "simple-git-hooks": "^2.7.0",
    "simple-github-release": "^1.0.0",
    "size-limit": "^7.0.8",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^4.5.5",
    "vite": "^4.3.9",
    "xmldom": "^0.6.0"
  },
  "readme": ""
}
